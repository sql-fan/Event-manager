<h1 class="text-white text-center pt-5 text-5xl">Your events</h1>
<div class="grid grid-cols-4 gap-4 px-10 pt-10">
  <% @events.each do |event| %>
    <div class="border-1 rounded-lg p-4">
      <h1 class="text-3xl text-center mb-5"><%= event.name %></h1>
      <p>When? <%= event.date %></p>
      <p>Where? Turku </p>
    </div>
  <% end %>
</div>